                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    OrderNumber AS NumeroOrden,
    CONVERT(char(10), OrderDate, 23) AS FechaOrden,
    CONVERT(char(10), ISNULL(ShipDate, '9999-12-31'), 23) AS FechaEnvio,
